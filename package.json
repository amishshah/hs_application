{
  "name": "hs-application",
  "version": "0.0.1",
  "description": "Application system for hackathons",
  "main": "app.ts",
  "engines": {
    "node": ">=9.0.0"
  },
  "scripts": {
    "build": "tsc -p .",
    "test": "ava",
    "test:coverage": "nyc ava",
    "test:watch": "DEBUG=ava:watcher npm test -- --watch --verbose",
    "start": "ts-node src/server.ts",
    "start:watch": "node ./node_modules/nodemon/bin/nodemon.js"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "!test/util/**/*"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "repository": {
    "type": "GitHub",
    "url": "https://github.com/unicsmcr/hs_application/tree/master"
  },
  "keywords": [
    "hackathon",
    "application",
    "registration"
  ],
  "author": "UniCS",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/morgan": "^1.7.37",
    "class-validator": "^0.10.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.1.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "inversify": "^5.0.1",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.18",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/supertest": "^2.0.8",
    "ava": "^2.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "sqlite3": "^4.1.0",
    "supertest": "^4.0.2",
    "ts-mockito": "^2.4.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0"
  }
}
